<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="1" />
		<loop startRow="1" items="result01" var="h"
			varType="cn.sp.xm.miyu.entity.Riddle">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">h.question</mapping>
				<mapping row="1" col="1">h.answer</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0" />
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>